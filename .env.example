DOMAIN_DNS=dns.test.site
DOMAIN_DASHBOARD=dashboard.test.site
FTLCONF_webserver_api_password=your_secure_password_here
TZ=Asia/Kolkata
# Standard DNS (port 53) - Remove if you don't want public DNS access
DNS_PORT_TCP=53:53/tcp    # TCP DNS - needed for large responses
DNS_PORT_UDP=53:53/udp    # UDP DNS - used by most clients

# Encrypted DNS (port 853) - Remove if you don't need dot
DOT_PORT=853:853/tcp      # DNS over TLS

ENABLE_DASHBOARD=true
ENABLE_DOH=true
